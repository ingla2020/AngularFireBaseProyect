{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist/AngularFireBaseProyect",
    "rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
